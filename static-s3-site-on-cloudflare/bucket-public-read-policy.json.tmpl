{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CloudFlareGetObject",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::${s3_bucket}/*",
            "Condition": {
                "IpAddress": {
                    "aws:SourceIp": ${src_ip_allow_list}
                }
            }
        }
    ]
}
